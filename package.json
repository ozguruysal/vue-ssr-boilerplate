{
  "name": "vue-ssr-boilerplate",
  "version": "1.0.0",
  "description": "A starting template for Vue.js server side rendering",
  "author": "Özgür Uysal (https://ozguruys.al)",
  "license": "MIT",
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --hide-modules",
    "lint": "eslint --ext .js,.vue src",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "compression": "^1.7.3",
    "express": "^4.16.4",
    "serve-favicon": "^2.5.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.4.7",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chokidar": "^2.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.14.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-vue": "^5.2.1",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "url-loader": "^1.1.2",
    "vue-jest": "^3.0.3",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.29.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
